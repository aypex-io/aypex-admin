<% content_for :breadcrumb do %>
<%= breadcrumb_builder(link_one_uri: aypex.admin_menus_path, link_one_text: I18n.t('aypex.admin.all_menus'), link_two_uri: aypex.edit_admin_menu_path(@menu), link_two_text: @menu.name, current_page_name: @menu_item.name) %>
<% end %>

<%= render partial: 'aypex/admin/shared/error_messages', locals: { target: @menu_item } %>

<%= form_with model: [:admin, @menu, @menu_item], data:{ controller: "form-state"} do |f| %>
  <%= render partial: 'form', locals: { f: f } %>
  <%= render partial: 'aypex/admin/shared/resource_actions' %>
<% end %>
