<% content_for :breadcrumb do %>
  <%= breadcrumb_builder(link_one_uri: aypex.admin_stores_path(@store), link_one_text: I18n.t("aypex.admin.stores"), current_page_name: I18n.t("aypex.admin.add_store")) %>
<% end %>

<%= form_with model: [:admin, @store], data: { turbo: false } do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'aypex/admin/shared/resource_actions', locals: { collection_url: aypex.admin_stores_path(@store) } %>
  </fieldset>
<% end %>
