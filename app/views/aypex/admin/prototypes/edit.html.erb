<% content_for :breadcrumb do %>
  <%= breadcrumb_builder(link_one_uri: aypex.admin_prototypes_path, link_one_text: I18n.t("aypex.admin.all_prototypes"), current_page_name: @prototype.name) %>
<% end %>

<div class="card">
  <%= form_with model: [:admin, @prototype] do |f| %>
    <div class="card-body">
       <%= render partial: 'form', locals: { f: f } %>
    </div>
    <div class="card-body text-center">
      <%= render partial: 'aypex/admin/shared/resource_actions' %>
    </div>
  <% end %>
</div>


