<% content_for :page_title do %>
  <%= breadcrumb_builder(link_one_uri: aypex.admin_stock_locations_path, link_one_text: I18n.t("aypex.admin.stock_locations"), current_page_name: @stock_location.name) %>
<% end %>

<%= render partial: 'aypex/admin/shared/error_messages', locals: { target: @stock_location } %>

<%= form_with model: [:admin, @stock_location] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'aypex/admin/shared/resource_actions' %>
  </fieldset>
<% end %>
