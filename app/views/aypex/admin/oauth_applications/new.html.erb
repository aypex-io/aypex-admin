<% content_for :page_title do %>
  <%= breadcrumb_builder(link_one_uri: aypex.admin_oauth_applications_url, link_one_text: I18n.t('aypex.admin.list'), current_page_name: I18n.t('aypex.admin.new')) %>
<% end %>

<%= turbo_frame_tag 'new_oauth_application' do %>
  <%= form_with model: [:admin, @oauth_application], data: { controller: "input-required"} do |f| %>
      <%= render partial: 'form', locals: { f: f } %>
      <%= render partial: 'aypex/admin/shared/resource_actions' %>
  <% end %>
<% end %>
